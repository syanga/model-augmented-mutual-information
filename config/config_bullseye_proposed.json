{
    "name": "Bullseye_vs_n_reg",
    "n_gpu": 1,
    "arch": {
        "type": "MappingFFGaussian",
        "args": {
            "xdim": 2,
            "ydim": 1,
            "num_features": 1,
            "dim_z_per": 2,
            "encoder_layers": 10,
            "decoder_layers": 10,
            "block_drop": 0.0
        }
    },
    "data_loader": {
        "type": "SupervisedDataLoader",
        "args": {
            "data_dir": "data/bullseye/vs_n/X_n_is_2900.h5",
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.0,
            "test_split": 0.0,
            "num_workers": 6,
            "seed": 0
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0.0,
            "amsgrad": true
        }
    },
    "loss": {
        "type": "gaussian_loss",
        "reg_coeff": 0.1,
        "jeffreys_batch": 10,
        "l1_coeff": 0.0,
        "pp_start_epoch": 0
    },
    "metrics": [],
    "lr_scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "patience": 10,
            "factor": 0.5
        }
    },
    "trainer": {
        "type": "TrainerPushPull",
        "epochs": 150,
        "save_dir": "saved/",
        "save_period": 150,
        "verbosity": 0,
        "monitor": "min loss",
        "early_stop": 10,
        "log_dir": "saved/runs",
        "plotz": true
    }
}